SELECT 
NF.NOT_IN_NUMERO,
ITNF.PRO_IN_CODIGO, 
PROD.PRO_ST_DESCRICAO,
NF.NOT_DT_EMISSAO,
ITNF.ITN_RE_QUANTIDADE,
ITNF.ITN_RE_VALORUNITARIO,
NF.NOT_RE_VALORMERCADORIA,
NF.NOT_RE_VALORTOTAL

FROM MGVEN.VEN_NOTAFISCAL NF, MGVEN.VEN_ITEMNOTAFISCAL ITNF, MGADM.EST_PRODUTOS PROD

WHERE NF.NOT_IN_CODIGO = ITNF.NOT_IN_CODIGO
AND ITNF.PRO_IN_CODIGO = PROD.PRO_IN_CODIGO
AND NF.NOT_IN_NUMERO IN (2431,42365,3635,3999,5987,5823,6831,8140,8774,11317,13273,12930,12812,14230,14112,14167,16439,16562,18428,19231,20205,20927,21081,21398,22153,24040,24279,26587,28001,30010,30065,30880,32111,33732,34763,38324,40029,40051,41834,1040)
