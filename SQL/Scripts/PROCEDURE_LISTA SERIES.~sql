
SELECT --  SELECIONA OS PERIODOS PARA EXEC DE MANEIRA DINAMICA.
MAX(CUS.CPE_IN_CODIGO) as ULT_PER,
MAX(CUS.CPE_IN_CODIGO)-2 AS PRI_PER
FROM MGCSC.CSC_MOVIMENTO CUS 


--==================================


SELECT PRO.PRO_IN_CODIGO,
TO_NUMBER(SUBSTR(PRO.PRO_ST_CODREAL,3,4)) AS PRO_IN_SERIE  -- LISTA DOS ITENS QUE POSSUEM LISTA DE MATERIAIS COM SUAS SERIES.
FROM MGADM.EST_PRODUTOS PRO, mgadm.Est_Itenslistamateriais LIS
WHERE PRO.PRO_IN_CODIGO = LIS.PRO_IN_CODIGO
GROUP BY PRO.PRO_IN_CODIGO, PRO.PRO_ST_CODREAL 
ORDER BY PRO.PRO_IN_CODIGO




