SELECT INS.* FROM MGADM.EST_MOVIMENTO MOV, MGADM.EST_LOTESMOVIMENTO LOTE_MOV, MGADM.EST_LOTESINSPECAO LOTE_INS, MGADM.EST_ITENSINSPECAO INS
WHERE MOV.MVT_IN_LANCAM = LOTE_MOV.MVT_IN_LANCAM
AND LOTE_MOV.MVL_ST_LOTEFORNE = LOTE_INS.LIN_ST_LOTEFORNE
AND LOTE_INS.INS_IN_SEQUENCIA = INS.INS_IN_SEQUENCIA
